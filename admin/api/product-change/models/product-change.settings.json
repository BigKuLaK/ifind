{
  "kind": "collectionType",
  "collectionName": "product_changes",
  "info": {
    "name": "Product Change",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": false
  },
  "attributes": {
    "state": {
      "type": "json"
    },
    "admin_user": {
      "plugin": "admin",
      "model": "user"
    },
    "date_time": {
      "type": "datetime"
    },
    "product": {
      "via": "product_changes",
      "model": "product"
    },
    "change_type": {
      "type": "enumeration",
      "enum": [
        "create",
        "update",
        "publish",
        "trash"
      ],
      "default": "update"
    }
  }
}

{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "name": "Product",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": false
  },
  "attributes": {
    "url": {
      "type": "string",
      "required": true,
      "regex": "(https?:\\/\\/.+)?"
    },
    "title": {
      "type": "string",
      "required": true
    },
    "position": {
      "type": "integer",
      "default": 0,
      "min": 0
    },
    "price": {
      "type": "decimal",
      "min": 0
    },
    "clicks_count": {
      "type": "integer",
      "min": 0,
      "default": 0
    },
    "categories": {
      "collection": "category",
      "via": "products",
      "dominant": true
    },
    "image": {
      "type": "string",
      "required": true
    },
    "region": {
      "model": "region",
      "via": "products"
    },
    "source": {
      "model": "source",
      "via": "products"
    },
    "website_tab": {
      "type": "string",
      "required": true
    }
  }
}

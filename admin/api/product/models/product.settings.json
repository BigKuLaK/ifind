{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "name": "Product",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": false
  },
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "position": {
      "type": "integer",
      "default": 0,
      "min": 0
    },
    "price": {
      "type": "decimal",
      "min": 0
    },
    "clicks_count": {
      "type": "integer",
      "min": 0,
      "default": 0
    },
    "categories": {
      "collection": "category",
      "via": "products",
      "dominant": true
    },
    "image": {
      "type": "string",
      "required": true
    },
    "region": {
      "via": "products",
      "model": "region"
    },
    "source": {
      "via": "products",
      "model": "source"
    },
    "website_tab": {
      "type": "string",
      "required": true
    },
    "url_list": {
      "type": "component",
      "repeatable": true,
      "component": "atoms.url-with-type",
      "required": false
    },
    "amazon_url": {
      "type": "string",
      "required": true
    },
    "details_html": {
      "type": "text"
    }
  }
}

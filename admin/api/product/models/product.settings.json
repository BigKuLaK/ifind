{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "name": "Product",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": false
  },
  "attributes": {
    "url": {
      "type": "string",
      "required": true,
      "regex": "(https?:\\/\\/.+)?"
    },
    "image": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "category": {
      "via": "products",
      "model": "category"
    },
    "title": {
      "type": "string",
      "required": true
    },
    "website_tab": {
      "type": "enumeration",
      "enum": [
        "tab_1",
        "tab_2"
      ],
      "required": true,
      "default": "tab_1"
    },
    "url_type": {
      "type": "enumeration",
      "enum": [
        "amazon",
        "alibaba",
        "ebay"
      ],
      "default": "amazon",
      "required": true
    },
    "position": {
      "type": "integer",
      "default": 0,
      "min": 0
    },
    "price": {
      "type": "decimal",
      "min": 0
    }
  }
}
